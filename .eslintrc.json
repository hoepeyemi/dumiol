{
  "extends": ["eslint:recommended"],
  "rules": {
    "no-unused-vars": "warn"
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "ignorePatterns": ["**/*.ts", "**/*.tsx"]
}
